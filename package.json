{
  "name": "opencti-react-chrome-extension",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "7.1.0",
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@mui/icons-material": "5.16.7",
    "@mui/material": "5.16.7",
    "@mui/x-data-grid": "6.20.4",
    "formik": "2.4.6",
    "formik-mui": "5.0.0-alpha.1",
    "ioc-extractor": "8.0.4",
    "ramda": "0.30.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rxjs": "7.8.1",
    "uuid": "9.0.1",
    "web-vitals": "^4.0.1",
    "webextension-polyfill": "^0.12.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "set INLINE_RUNTIME_CHUNK=false&&craco build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "package": "(cd build && zip -r ../opencti-browser-extension.zip . -x \"*.DS_Store\" -x \"__MACOSX\")"
